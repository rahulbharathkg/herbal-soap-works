# Copy to .env and fill in values for production

# JWT secret for signing tokens
JWT_SECRET=replace_with_a_strong_secret

# Use SQLite for local dev; set to false in production to use Postgres
USE_SQLITE=true

# Postgres / Supabase settings (used if USE_SQLITE=false)
DB_HOST=localhost
DB_PORT=5432
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=herbalsoapworks

# Optional admin seed
SEED_ADMIN_EMAIL=admin@herbalsoapworks.local
SEED_ADMIN_PASSWORD=adminpass
