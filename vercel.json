{
    "version": 2,
    "outputDirectory": "frontend/build",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/((?!api/|static/|images/|favicon.ico|manifest.json|logo.*.png).*)",
            "destination": "/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}